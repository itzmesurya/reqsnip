{
  "name": "reqsnip",
  "displayName": "reqsnip",
  "description": "require code snippet",
  "version": "0.0.1",
  "publisher": "surya",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.addReq",
    "onCommand:extension.addGulpTask"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.addReq",
        "title": "add Require"
      },
      {
        "command": "extension.addGulpTask",
        "title": "add Gulp Task"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "change-case": "^3.0.1"
  }
}
